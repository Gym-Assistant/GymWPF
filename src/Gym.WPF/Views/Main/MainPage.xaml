<Page x:Class="Gym.WPF.Views.Main.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:Gym.WPF.Views.Main"
      xmlns:vm="clr-namespace:Gym.MVVM.ViewModels.Main;assembly=Gym.MVVM"
      mc:Ignorable="d"
      d:DataContext="{d:DesignInstance Type=vm:MainPageViewModel}"
      Title="MainPage" Height="450" Width="800">
  <Grid>
    <TextBlock x:Name="HelloWorld" Text="{Binding Path=HelloWorld}" Height="30" ></TextBlock>
  </Grid>
</Page>

